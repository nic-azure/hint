{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js",
      "!dist/tests/**/fixtures/**/*.js",
      "!dist/tests/helpers/**/*.js"
    ],
    "timeout": "1m"
  },
  "browserslist": "last 2 chrome versions, last 2 firefox versions",
  "bundleSize": 680000,
  "description": "webhint browser extension",
  "devDependencies": {
    "@hint/hint-axe": "^4.3.3",
    "@hint/hint-button-type": "^2.3.2",
