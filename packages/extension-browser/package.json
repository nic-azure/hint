    "@hint/hint-no-html-only-headers": "^2.4.2",
    "@hint/hint-no-http-redirects": "^2.4.2",
    "@hint/hint-no-protocol-relative-urls": "^2.4.2",
    "@hint/hint-no-vulnerable-javascript-libraries": "^2.11.2",
    "@hint/hint-scoped-svg-styles": "^1.3.2",
    "@hint/hint-sri": "^3.3.2",
    "@hint/hint-stylesheet-limits": "^3.3.2",
    "@hint/hint-validate-set-cookie-header": "^2.4.2",
    "@hint/hint-x-content-type-options": "^3.3.2",
    "@hint/parser-css": "^3.0.17",
    "@hint/parser-html": "^3.0.13",
    "@hint/parser-javascript": "^3.1.3",
    "@hint/parser-manifest": "^2.2.16",
    "@hint/utils": "^7.0.2",
    "@hint/utils-create-server": "^3.4.2",
    "@hint/utils-dom": "^2.0.0",
    "@hint/utils-fs": "^1.0.1",
    "@hint/utils-i18n": "^1.0.1",
    "@hint/utils-telemetry": "^1.0.1",
    "@hint/utils-types": "^1.0.1",
    "@types/chrome": "^0.0.92",
    "@types/har-format": "^1.2.4",
    "@types/is-ci": "^2.0.0",
    "@types/jsdom": "^16.1.0",
    "@types/node": "^12.12.14",
    "@types/proxyquire": "^1.3.28",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.12.0",
    "ava": "^3.4.0",
    "axe-core": "3.4.1",
    "browserslist": "^4.9.1",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-react-hooks": "^2.5.0",
    "focus-visible": "^5.0.2",
    "glob": "^7.1.6",
    "globby": "^11.0.0",
    "hint": "^6.0.4",
    "is-ci": "^2.0.0",
    "jsdom": "^16.2.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "proxyquire": "^2.1.3",
    "puppeteer": "^2.1.1",
    "raw-loader": "^4.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-syntax-highlighter": "^12.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^4.0.0",
    "terser-webpack-plugin": "^2.3.4",
    "typed-css-modules": "^0.6.3",
    "typescript": "^3.8.3",
